<?xml version="1.0" encoding="UTF-8"?>
<Language casesensitive="true">
    <Keywords>
        <Keyword>abstract</Keyword>
        <Keyword>as</Keyword>
        <Keyword>base</Keyword>
        <Keyword>bool</Keyword>
        <Keyword>break</Keyword>
        <Keyword>byte</Keyword>
        <Keyword>case</Keyword>
        <Keyword>catch</Keyword>
        <Keyword>char</Keyword>
        <Keyword>checked</Keyword>
        <Keyword>class</Keyword>
        <Keyword>const</Keyword>
        <Keyword>continue</Keyword>
        <Keyword>decimal</Keyword>
        <Keyword>default</Keyword>
        <Keyword>delegate</Keyword>
        <Keyword>do</Keyword>
        <Keyword>double</Keyword>
        <Keyword>else</Keyword>
        <Keyword>enum</Keyword>
        <Keyword>event</Keyword>
        <Keyword>explicit</Keyword>
        <Keyword>extern</Keyword>
        <Keyword>false</Keyword>
        <Keyword>finally</Keyword>
        <Keyword>fixed</Keyword>
        <Keyword>float</Keyword>
        <Keyword>for</Keyword>
        <Keyword>foreach</Keyword>
        <Keyword>goto</Keyword>
        <Keyword>if</Keyword>
        <Keyword>implicit</Keyword>
        <Keyword>in</Keyword>
        <Keyword>int</Keyword>
        <Keyword>interface</Keyword>
        <Keyword>internal</Keyword>
        <Keyword>is</Keyword>
        <Keyword>lock</Keyword>
        <Keyword>long</Keyword>
        <Keyword>namespace</Keyword>
        <Keyword>new</Keyword>
        <Keyword>null</Keyword>
        <Keyword>object</Keyword>
        <Keyword>operator</Keyword>
        <Keyword>out</Keyword>
        <Keyword>override</Keyword>
        <Keyword>params</Keyword>
        <Keyword>private</Keyword>
        <Keyword>protected</Keyword>
        <Keyword>public</Keyword>
        <Keyword>readonly</Keyword>
        <Keyword>ref</Keyword>
        <Keyword>return</Keyword>
        <Keyword>sbyte</Keyword>
        <Keyword>sealed</Keyword>
        <Keyword>short</Keyword>
        <Keyword>sizeof</Keyword>
        <Keyword>stackalloc</Keyword>
        <Keyword>static</Keyword>
        <Keyword>string</Keyword>
        <Keyword>struct</Keyword>
        <Keyword>switch</Keyword>
        <Keyword>this</Keyword>
        <Keyword>throw</Keyword>
        <Keyword>true</Keyword>
        <Keyword>try</Keyword>
        <Keyword>typeof</Keyword>
        <Keyword>uint</Keyword>
        <Keyword>ulong</Keyword>
        <Keyword>unchecked</Keyword>
        <Keyword>unsafe</Keyword>
        <Keyword>ushort</Keyword>
        <Keyword>using</Keyword>
        <Keyword>virtual</Keyword>
        <Keyword>valatile</Keyword>
        <Keyword>void</Keyword>
        <Keyword>while</Keyword>
        <Keyword>ascending</Keyword>
        <Keyword>descending</Keyword>
        <Keyword>from</Keyword>
        <Keyword>get</Keyword>
        <Keyword>group</Keyword>
        <Keyword>into</Keyword>
        <Keyword>join</Keyword>
        <Keyword>let</Keyword>
        <Keyword>orderby</Keyword>
        <Keyword>partial</Keyword>
        <Keyword>select</Keyword>
        <Keyword>set</Keyword>
        <Keyword>value</Keyword>
        <Keyword>var</Keyword>
        <Keyword>where</Keyword>
        <Keyword>yield</Keyword>
    </Keywords>
    <Contains>
        <Contain name="xmlDocTag|ref1">
            <Begin>///|&lt;!--|--&gt;</Begin>
        </Contain>
        <Contain name="xmlDocTag|ref2">
            <Begin>&lt;/?</Begin>
            <End>&gt;</End>
        </Contain>
        <Contain name="comment">
            <Begin>///</Begin>
            <End>$</End>
            <ReturnBegin>true</ReturnBegin>
            <RefContains>
                <RefContain>xmlDocTag|ref1</RefContain>
                <RefContain>xmlDocTag|ref2</RefContain>
            </RefContains>
        </Contain>
        <Contain name="comment">
            <Begin>//</Begin>
            <End>$</End>
        </Contain>
        <Contain name="comment">
            <Begin>/\*</Begin>
            <End>\*/</End>
        </Contain>
        <Contain name="preprocessor">
            <Begin>#</Begin>
            <End>$</End>
            <Keywords>
                <Keyword>if</Keyword>
                <Keyword>else</Keyword>
                <Keyword>elif</Keyword>
                <Keyword>endif</Keyword>
                <Keyword>define</Keyword>
                <Keyword>undef</Keyword>
                <Keyword>warning</Keyword>
                <Keyword>error</Keyword>
                <Keyword>line</Keyword>
                <Keyword>region</Keyword>
                <Keyword>endregion</Keyword>
                <Keyword>pragma</Keyword>
                <Keyword>checksum</Keyword>
            </Keywords>
        </Contain>
        <Contain name="noname">
            <ShowClassName>false</ShowClassName>
            <Begin>""</Begin>
        </Contain>
        <Contain name="string">
            <Begin>@</Begin>
            <End>"</End>
        </Contain>
        <Contain name="backslash_escape">
            <ShowClassName>false</ShowClassName>
            <Begin>\\[\s\S]</Begin>
        </Contain>
        <Contain name="string|apos">
            <Begin>'</Begin>
            <End>'</End>
            <Illegal>\n</Illegal>
            <Relevance>0</Relevance>
            <RefContains>
                <RefContain>backslash_escape</RefContain>
            </RefContains>
        </Contain>
        <Contain name="string|quote">
            <Begin>"</Begin>
            <End>"</End>
            <Illegal>\n</Illegal>
            <Relevance>0</Relevance>
            <RefContains>
                <RefContain>backslash_escape</RefContain>
            </RefContains>
        </Contain>
        <Contain name="number">
            <Begin>(\b0[xX][a-fA-F0-9]+|(\b\d+(\.\d*)?|\.\d+)([eE][-+]?\d+)?)</Begin>
        </Contain>
    </Contains>
</Language>
