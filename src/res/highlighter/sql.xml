<?xml version="1.0" encoding="UTF-8"?>
<Language casesensitive="false">
    <Contains>
        <Contain name="noname1">
            <ShowClassName>false</ShowClassName>
            <Begin>''</Begin>
        </Contain>
        <Contain name="noname2">
            <ShowClassName>false</ShowClassName>
            <Begin>""</Begin>
        </Contain>
        <Contain name="backslash_escape">
            <ShowClassName>false</ShowClassName>
            <Begin>\\[\s\S]</Begin>
        </Contain>
        <Contain name="string|ref1">
            <Begin>'</Begin>
            <End>'</End>
            <RefContains>
                <RefContain>noname1</RefContain>
                <RefContain>backslash_escape</RefContain>
            </RefContains>
        </Contain>
        <Contain name="string|ref2">
            <Begin>"</Begin>
            <End>"</End>
            <RefContains>
                <RefContain>noname2</RefContain>
                <RefContain>backslash_escape</RefContain>
            </RefContains>
        </Contain>
        <Contain name="string|ref3">
            <Begin>`</Begin>
            <End>`</End>
            <RefContains>
                <RefContain>backslash_escape</RefContain>
            </RefContains>
        </Contain>
        <Contain name="number">
            <Begin>(\b0[xX][a-fA-F0-9]+|(\b\d+(\.\d*)?|\.\d+)([eE][-+]?\d+)?)</Begin>
        </Contain>
        <!--main-->
