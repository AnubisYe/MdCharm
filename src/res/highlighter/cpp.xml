    <Contains>
        <Contain name="comment">
            <Begin>//</Begin>
            <End>$</End>
        </Contain>
        <Contain name="comment">
            <Begin>/\*</Begin>
            <End>\*/</End>
        </Contain>
        <Contain name="backslash_escape">
            <ShowClassName>false</ShowClassName>
            <Begin>\\[\s\S]</Begin>
            <Relevance>0</Relevance>
        </Contain>
        <Contain name="string">
            <Begin>"</Begin>
            <End>"</End>
            <Illegal>\n</Illegal>
            <Relevance>0</Relevance>
            <RefContains>
                <RefContain>backslash_escape</RefContain>
            </RefContains>
        </Contain>
        <Contain name="string">
            <Begin>\'\\?.</Begin>
            <End>\'</End>
            <Illegal>.</Illegal>
        </Contain>
        <Contain name="number">
            <Begin>\b(\d+(\.\d*)?|\.\d+)(u|U|l|L|ul|UL|f|F)</Begin>
        </Contain>
        <Contain name="number">
            <Begin>(\b0[xX][a-fA-F0-9]+|(\b\d+(\.\d*)?|\.\d+)([eE][-+]?\d+)?)</Begin>
            <Relevance>0</Relevance>
        </Contain>
        <Contain name="preprocessor">
            <Begin>#</Begin>
            <End>$</End>
        </Contain>
        <Contain name="stl_container">
            <Begin>\b(deque|list|queue|stack|vector|map|set|bitset|multiset|multimap|unordered_map|unordered_set|unordered_multiset|unordered_multimap|array)\s*&lt;</Begin>
            <End>&gt;</End>
            <Keywords refLanguageKeywords="true"/>
            <RefContains refSelf="true"/>
        </Contain>
    </Contains>
</Language>
